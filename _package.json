{
  "name": "patrashopweb",
  "version": "0.0.0",
  "description": "PATRAのWebショッピングサイト",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node build/server.js",
    "predev": "npm run prebuild && npm run build:server",
    "dev": "env NODE_ENV=development && npm run build:ts -- --watch --devtool=inline-source-map & npm run build:server -- --watch & nodemon build/server.js",
    "prebuild": "rm -rf public",
    "build": "npm run build:server && npm run build:ts --mode=production",
    "build:ts": "webpack -p --mode=development",
    "build:server": "tsc -p tsconfig.json",
    "lint": "tslint -c tslint.json -p tsconfig.json -t verbose 'src/**/*.{ts,tsx}'",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chotchy-inc/PATRASHOP-Web.git"
  },
  "author": "konojunya <junya.developer[at]gmail.com>",
  "bugs": {
    "url": "https://github.com/chotchy-inc/PATRASHOP-Web/issues"
  },
  "homepage": "https://github.com/chotchy-inc/PATRASHOP-Web#readme",
